                    _comments.stop(!0, !0).slideUp(200);
                } else {
                    _self.text("ÑÐºÑ€Ñ‹Ñ‚ÑŒ Ð¾Ñ‚Ð²ÐµÑ‚Ñ‹");
                    _self.data("show", 1);
                    _comments.slideDown(200);
                }
            });
