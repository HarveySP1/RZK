    var bindFavoritesEvents = function () {
        var addFavSumitButtonObj = $("#addcat-fav-addbt");
        var addFavButtonObj = $(".b-sideactions__fav .add-favorite");
        addFavSumitButtonObj.on("click", function (e) {
